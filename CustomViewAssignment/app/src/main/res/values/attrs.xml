<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedCornerTextView">
        <attr name="cornerRadius" format="dimension" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="strokeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularProgressBar">
        <attr name="backColor" format="color" />
        <attr name="frontColor" format="color" />
        <attr name="progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="StraightProgressBar">
        <attr name="progressNumber" format="integer"/>
        <attr name="progressBarColor" format="color"/>
        <attr name="backgroundLineColor" format="color"/>
    </declare-styleable>

</resources>